{% extends "hepiaThanatosBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Voir un réseau de neurones{% endblock %}

{% block body %}
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Voir un réseau de neurones</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Résumé
                    </div>
                    <div class="panel-body">
                        <table width="100%" class="table table-bordered">
                            <tr>
                                <th>Titre</th>
                                <td>{{ network.title }}</td>
                            </tr>
                            <tr>
                                <th>Description</th>
                                <td>{{ network.Description }}</td>
                            </tr>
                            <tr>
                                <th>Architecture</th>
                                <td>{{ network.network }}</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
